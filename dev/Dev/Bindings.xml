<Bindings>
	<Binding name="DEV_RELOADUI" header="DEV">
		_Dev.ReloadUIBinding();
	</Binding>
	<Binding name="DEV_OPENCHATSCRIPT">
		_Dev.OpenChatScriptBinding();
	</Binding>
	<Binding name="DEV_FRAMESTOGGLE">
		_Dev.Frames.Toggle();
	</Binding>

	<ModifiedClick action="DEV_ENABLECONSOLE" default="CTRL"/>
	<ModifiedClick action="DEV_FRAMES_INTERACTIVE" default="ALT"/>
	<ModifiedClick action="DEV_FRAMES_OUTLINE" default="ALT-BUTTON1"/>
	<ModifiedClick action="DEV_FRAMES_NAME" default="ALT-BUTTON2"/>
	<ModifiedClick action="DEV_FRAMES_DUMP" default="ALT-BUTTON3"/>
</Bindings>