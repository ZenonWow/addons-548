<Bindings>
	<Binding name="KEY NPCScan.Overlay.Button:LeftButton" header="NPCSCAN_OVERLAY">
				_G.NPCScan.Overlay.WorldMapKey_ToggleOnClick()
	</Binding>

	<Binding name="PATH NPCScan.Overlay.Button:LeftButton">
		_G.NPCScan.Overlay.WorldMapPaths_ToggleOnClick()
	</Binding>

	<Binding name="MINIMAP_PATH NPCScan.Overlay.Button:LeftButton">
		Minimap_Toggle()
	</Binding>


	<ModifiedClick action="NPCSCAN_BUTTONDRAG" default="CTRL"/>
</Bindings>