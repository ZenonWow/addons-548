<Bindings>
	<Binding name="EXAMINER_OPEN" header="EXAMINER">
		Examiner:OpenSimple();
	</Binding>
	<Binding name="EXAMINER_TARGET">
		if (Examiner:IsShown()) then
			HideUIPanel(Examiner);
		else
			Examiner:DoInspect("target");
		end
	</Binding>
	<Binding name="EXAMINER_MOUSEOVER">
		Examiner:InspectMouseover();		
	</Binding>
	
	<ModifiedClick action="SHOW_ITEMLEVEL" default="ALT"/>
	<ModifiedClick action="DRESSUP_NO_TMOG" default="ALT"/>
	
</Bindings>


